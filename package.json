{
  "name": "@a11ywatch/web",
  "version": "0.4.1",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:vercel": "NODE_OPTIONS='--inspect' vercel dev",
    "start": "next start -p ${PORT:=3000}",
    "build": "next build",
    "build:wasm": "cd wasm && wasm-pack build --target bundler",
    "lint": "next lint",
    "fix": "prettier --write '**/*.{js,jsx}'",
    "clean": "npm run fix && npm run lint",
    "postbuild": "next-sitemap",
    "test": "jest",
    "test:wasm": "cd wasm && wasm-pack test --node",
    "pub:wasm": "npm run build:wasm && cd wasm/pkg && npm publish",
    "test:ci": "jest --forceExit --ci --detectOpenHandles",
    "test:e2e": "playwright test"
  },
